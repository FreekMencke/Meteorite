{
  "extends": "../../tslint.json",
  "rules": {
    "component-selector": [
      true,
      "element",
      "mtr",
      "kebab-case"
    ],
    "directive-selector": [
      true,
      "attribute",
      "mtr",
      "camelCase"
    ],
    "prefer-on-push-component-change-detection": true
  }
}
